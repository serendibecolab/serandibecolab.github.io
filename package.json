    "@types/crypto-js": "^4.2.2",
    "@types/lozad": "^1.16.4",
    "crypto-js": "^4.2.0",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "reading-time": "^1.5.0",
