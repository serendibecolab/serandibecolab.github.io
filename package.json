{
  "name": "yukina",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev --host",
    "preview": "astro preview",
    "astro": "astro",
    "start": "python3 -m http.server --directory dist 3000",
    "build:gh-pages": "bash -lc 'REPO=${GITHUB_PAGES_REPO:-${GITHUB_REPOSITORY:-}}; if [ -z \"$REPO\" ]; then echo \"Usage: GITHUB_PAGES_REPO=owner/repo pnpm run build:gh-pages\"; exit 1; fi; OWNER=${REPO%%/*}; REPO_NAME=${REPO##*/}; if [ \"$REPO_NAME\" = \"$OWNER.github.io\" ]; then GITHUB_PAGES_BASE=${GITHUB_PAGES_BASE:-/}; GITHUB_PAGES_URL=${GITHUB_PAGES_URL:-https://$OWNER.github.io}; else GITHUB_PAGES_BASE=${GITHUB_PAGES_BASE:-/$(basename \"$REPO\")/}; GITHUB_PAGES_URL=${GITHUB_PAGES_URL:-https://$OWNER.github.io$GITHUB_PAGES_BASE}; fi; echo \"Building with GITHUB_PAGES_BASE=$GITHUB_PAGES_BASE GITHUB_PAGES_URL=$GITHUB_PAGES_URL\"; GITHUB_PAGES_BASE=\"$GITHUB_PAGES_BASE\" GITHUB_PAGES_URL=\"$GITHUB_PAGES_URL\" pnpm run build'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^7.0.3",
    "@astrojs/tailwind": "^5.1.4",
    "@swup/astro": "^1.5.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "astro": "^5.3.0",
    "astro-icon": "^1.1.5",
    "astro-pagefind": "^1.7.0",
    "lozad": "^1.16.0",
    "overlayscrollbars": "^2.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "^0.34.5",
    "svelte": "^5.17.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "devDependencies": {
