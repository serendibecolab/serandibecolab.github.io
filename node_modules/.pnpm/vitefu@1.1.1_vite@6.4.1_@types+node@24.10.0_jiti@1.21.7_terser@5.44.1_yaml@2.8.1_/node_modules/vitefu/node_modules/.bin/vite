#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/banumth/Projects/yukina/node_modules/.pnpm/vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/banumth/Projects/yukina/node_modules/.pnpm/vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules/vite/node_modules:/home/banumth/Projects/yukina/node_modules/.pnpm/vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules:/home/banumth/Projects/yukina/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/banumth/Projects/yukina/node_modules/.pnpm/vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/banumth/Projects/yukina/node_modules/.pnpm/vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules/vite/node_modules:/home/banumth/Projects/yukina/node_modules/.pnpm/vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules:/home/banumth/Projects/yukina/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@6.4.1_@types+node@24.10.0_jiti@1.21.7_terser@5.44.1_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
